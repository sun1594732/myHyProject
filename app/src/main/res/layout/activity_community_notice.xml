<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_community_notice"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/toolbar"/>
    <com.hyzs.onekeyhelp.widget.view.VRefresh
        android:id="@+id/activity_community_notice_refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/activity_community_notice_lv"
        android:dividerHeight="@dimen/x1"
        android:divider="@color/color_f4"
        android:background="@color/color_f"
        android:scrollbars="none"/>
    </com.hyzs.onekeyhelp.widget.view.VRefresh>
</LinearLayout>
