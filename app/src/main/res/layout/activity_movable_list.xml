<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_f"
    android:orientation="vertical">

    <include layout="@layout/toolbar" />


    <com.hyzs.onekeyhelp.widget.view.VRefresh
        android:id="@+id/activity_movable_list_refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ListView
            android:id="@+id/activity_movable_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@color/color_f4"
            android:dividerHeight="@dimen/x6"
            android:scrollbars="none"/>

    </com.hyzs.onekeyhelp.widget.view.VRefresh>
</LinearLayout>